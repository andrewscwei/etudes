{
  "name": "etudes",
  "version": "6.1.1",
  "description": "A study of headless React components",
  "type": "module",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "prepages": "rimraf .gh-pages",
    "pages": "vite build -c demo/vite.config.ts",
    "dev": "concurrently \"tsc -w\" \"wait-on -t 30s lib && vite dev -c demo/vite.config.ts --port 8080\"",
    "test": "echo \"No tests yet :)\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "components",
    "hooks",
    "operators",
    "providers",
    "utils"
  ],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "eslint": "^9.10.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.45",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.1",
    "tailwindcss": "^3.4.10",
    "tailwindcss-safe-area": "^0.6.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.4",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^4.5.0",
    "interactjs": "^1.10.27",
    "resize-observer-polyfill": "^1.5.1",
    "spase": "^9.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "optionalDependencies": {
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2"
  }
}
