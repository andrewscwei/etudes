{
  "name": "etudes",
  "version": "0.22.4",
  "description": "A study of styled React components",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc -p tsconfig.json",
    "pages": "rimraf .gh-pages && webpack --config demo/config/build.conf.ts --port 8080",
    "demo": "NODE_ENV=development webpack-dev-server --display-error-details --hot --config demo/config/build.conf.ts",
    "lint": "tslint \"src/**/*\" \"demo/**/*\" --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/debug": "^4.1.5",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-hot-loader": "^4.1.1",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.12",
    "@types/webpack-env": "^1.15.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "debug": "^4.1.1",
    "dirty-dom": "^4.3.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "interactjs": "1.9.4",
    "promptu": "^2.5.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
