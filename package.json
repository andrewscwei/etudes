{
  "name": "etudes",
  "version": "2.1.0",
  "description": "A study of headless React components",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "pages": "rimraf .gh-pages && webpack --config demo/config/build.conf.ts",
    "demo": "npm run clean && concurrently \"cross-env NODE_ENV=production tsc -w --diagnostics false\" \"wait-on lib && cross-env NODE_ENV=development webpack-cli serve --mode development --hot --config demo/config/build.conf.ts\"",
    "test": "echo \"No tests yet :)\"",
    "lint": "eslint --ext .ts --ext .tsx src demo",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node-polyglot": "^2.4.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.26",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.33.0",
    "html-webpack-plugin": "^5.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
    "rimraf": "^4.1.2",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "fast-deep-equal": "^3.1.3",
    "interactjs": "^1.10.17",
    "node-polyglot": "^2.5.0",
    "react-transition-group": "^4.4.5",
    "resize-observer-polyfill": "^1.5.1",
    "spase": "^6.5.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "optionalDependencies": {
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0"
  }
}
