{
  "name": "etudes",
  "version": "5.3.0",
  "description": "A study of headless React components",
  "type": "module",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "pages": "rimraf .gh-pages && webpack --config demo/config/build.conf.ts",
    "dev": "concurrently \"tsc -w\" \"wait-on -t 30s lib && vite dev -c demo/vite.config.ts --port 8080\"",
    "test": "echo \"No tests yet :)\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "components",
    "hooks",
    "operators",
    "providers",
    "utils"
  ],
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.7.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "debug": "^4.3.6",
    "eslint": "^9.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.0",
    "styled-components": "^6.1.13",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3",
    "vite-plugin-dts": "^4.1.0",
    "wait-on": "^8.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^4.5.0",
    "interactjs": "^1.10.27",
    "resize-observer-polyfill": "^1.5.1",
    "spase": "^8.0.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "optionalDependencies": {
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1"
  }
}
