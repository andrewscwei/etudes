{
  "name": "etudes",
  "version": "1.0.0",
  "description": "A study of styled React components",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "pages": "rimraf .gh-pages && webpack --config demo/config/build.conf.ts",
    "demo": "npm run clean && concurrently \"cross-env NODE_ENV=production tsc -w\" \"wait-on lib && cross-env NODE_ENV=development webpack-cli serve --mode development --hot --config demo/config/build.conf.ts\"",
    "test": "echo \"No tests yet :)\"",
    "lint": "eslint --ext .ts --ext .tsx src demo",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.25",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.19.0",
    "file-loader": "^6.2.0",
