{
  "name": "etudes",
  "version": "4.0.0-rc.22",
  "description": "A study of headless React components",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint",
    "build": "npm run clean && cross-env NODE_ENV=production tsc",
    "pages": "rimraf .gh-pages && webpack --config demo/config/build.conf.ts",
    "demo": "npm run clean && concurrently \"cross-env NODE_ENV=production tsc -w --diagnostics false\" \"wait-on lib && cross-env NODE_ENV=development webpack-cli serve --mode development --hot --config demo/config/build.conf.ts\"",
    "test": "echo \"No tests yet :)\"",
    "lint": "eslint --ext .ts --ext .tsx src demo",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "components",
    "hooks",
    "operators",
    "providers",
    "utils"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/debug": "^4.1.12",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/node-polyglot": "^2.4.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-transition-group": "^4.4.9",
    "@types/styled-components": "^5.1.30",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.53.0",
    "fast-xml-parser": "^4.3.2",
    "html-webpack-plugin": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "rimraf": "^5.0.5",
    "styled-components": "^6.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wait-on": "^7.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "fast-deep-equal": "^3.1.3",
    "interactjs": "^1.10.20",
    "react-transition-group": "^4.4.5",
    "resize-observer-polyfill": "^1.5.1",
    "spase": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "optionalDependencies": {
    "fast-xml-parser": "^4.3.2",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0"
  }
