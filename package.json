    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/debug": "^4.1.5",
    "@types/html-webpack-plugin": "^3.2.4",
