{
  "name": "etudes",
  "version": "6.3.0",
  "description": "A study of headless React components",
  "type": "module",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "prepages": "rimraf .gh-pages",
    "pages": "vite build -c demo/vite.config.ts",
    "dev": "concurrently \"tsc -w\" \"wait-on -t 30s lib && vite dev -c demo/vite.config.ts --port 8080\"",
    "test": "npm run unit",
    "unit": "vitest run --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "components",
    "hooks",
    "operators",
    "providers",
    "utils"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.3",
    "tailwindcss": "^3.4.14",
    "tailwindcss-safe-area": "^0.6.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.3",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^4.5.0",
    "interactjs": "^1.10.27",
    "resize-observer-polyfill": "^1.5.1",
    "spase": "^9.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "optionalDependencies": {
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
