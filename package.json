{
  "name": "etudes",
  "version": "8.0.0",
  "description": "A study of headless React components",
  "type": "module",
  "main": "./build/etudes.umd.cjs",
  "module": "./build/etudes.js",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean && npm run lint",
    "build": "vite build",
    "prepages": "rimraf .gh-pages",
    "pages": "vite build -c demo/vite.config.ts",
    "dev": "concurrently \"vite build -w\" \"wait-on build && vite dev -c demo/vite.config.ts --port 8080\"",
    "test": "npm run unit",
    "unit": "vitest run --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/etudes.git"
  },
  "keywords": [
    "etudes",
    "react",
    "ui",
    "components"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tailwindcss/vite": "^4.0.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.8",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "tailwindcss": "^4.0.14",
    "tailwindcss-safe-area": "^0.6.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^5.0.8",
    "interactjs": "^1.10.27",
    "spase": "^10.0.1"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-router": "^7.1.3"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-router": {
      "optional": true
    }
  }
}
